    <servlet-class>ServletSessionCookies.session</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newSession</servlet-name>
    <url-pattern>/newSession</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>newCookie</display-name>
    <servlet-name>newCookie</servlet-name>
    <servlet-class>ServletCookies.cookies</servlet-class>
